<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties resource="generatorConfig.properties"></properties>

    <context id="context1">

        <property name="autoDelimitKeywords" value="true" />
        <property name="beginningDelimiter" value="`" />
        <property name="endingDelimiter" value="`" />

        <plugin type="com.webbleen.ssm.mybatis.plugin.LombokPlugin"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <plugin type="com.webbleen.ssm.mybatis.plugin.MysqlPagePlugin"/>

        <commentGenerator type="com.webbleen.ssm.mybatis.comment.MyCommentGenerator"></commentGenerator>

        <jdbcConnection
                driverClass="${jdbc.driver}"
                connectionURL="${jdbc.url}"
                userId="${jdbc.username}"
                password="${jdbc.password}">
            <property name="nullCatalogMeansCurrent" value="true" />
        </jdbcConnection>
        
        <javaModelGenerator targetPackage="${model.package}"
         targetProject="${target.project}/java"/>
        <sqlMapGenerator targetPackage="${xml.mapper.package}"
         targetProject="${target.project}/resources"/>
        <javaClientGenerator targetPackage="${dao.package}"
         targetProject="${target.project}/java" type="XMLMAPPER"/>

        <table tableName="w_user" domainObjectName="User" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false" />

    </context>
</generatorConfiguration>